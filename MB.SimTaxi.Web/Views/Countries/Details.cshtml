@model MB.SimTaxi.Web.Data.Entities.Country

@{
    ViewData["Title"] = $"{Model.Title} Details";
}

<h1><span class="text-primary">@Model.Title</span> Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Title)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Title)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Code)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Code)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-secondary" asp-action="Index">Back</a>
    <a class="btn btn-primary ms-2" asp-action="Edit" asp-route-id="@Model?.Id">Edit</a>
</div>
